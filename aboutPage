from tkinter import *

scrn = Tk()

nameLabel = Label(text="Evan Rothenbacher")
nameLabel.grid()

yearLabel = Label(text="Year: 2022")
yearLabel.grid()

toolLabel = Label(text="Tools Used: tkinter, vscode, python")
toolLabel.grid()

classLabel = Label(text="Intro To Programming")
classLabel.grid()

teacherLabel = Label(text="Mr. Osowski")
teacherLabel.grid()

scrn.mainloop()